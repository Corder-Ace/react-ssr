{
    "compilerOptions": {
        "outDir": "./dist/", //输出目录
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "moduleResolution":"node",
        "allowSyntheticDefaultImports": true,
        "forceConsistentCasingInFileNames": false,
        "experimentalDecorators": true,
        "allowJs": true,
    },
    "include": [
        "./client/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}